{
  "name": "next-ts-starter",
  "description": "Next Typescript Starter Code",
  "private": true,
  "license": "MIT",
  "author": "Aro Andriamaro <ni.aro.aina@gmail.com>",
  "keywords": [
    "next js template",
    "react template",
    "react typescript"
  ],
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "lint": "eslint --fix --cache .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,sass,md,mdx,html,json}": [
      "prettier --write --ignore-unknown"
    ],
    "*.{jsx,js,ts,tsx}": [
      "eslint --fix --cache"
    ]
  },
  "dependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@stitches/react": "^1.2.8",
    "next": "^12.2.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-query": "^3.39.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
