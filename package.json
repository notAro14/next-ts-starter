{
  "name": "next-ts-starter",
  "description": "Next Typescript Starter Code",
  "private": true,
  "license": "MIT",
  "author": "Aro Andriamaro <ni.aro.aina@gmail.com>",
  "keywords": [
    "next js template",
    "react template",
    "react typescript"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --cache .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,sass,md,mdx,html,json}": [
      "prettier --write --ignore-unknown"
    ],
    "*.{jsx,js,ts,tsx}": [
      "eslint --fix --cache"
    ]
  },
  "dependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-checkbox": "^1.0.0",
    "@radix-ui/react-icons": "^1.1.1",
    "@radix-ui/react-select": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@stitches/react": "^1.2.8",
    "@uiball/loaders": "^1.2.6",
    "axios": "^0.27.2",
    "next": "^12.3.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "tinycolor2": "^1.4.2"
