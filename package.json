{
  "name": "next-ts-starter",
  "description": "Next Typescript Starter Code",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "format": "prettier --check --ignore-unknown .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,sass,md,mdx,html,json}": [
      "prettier --check --ignore-unknown"
    ],
    "*.{jsx,js,ts,tsx}": [
      "eslint --cache"
    ]
  },
  "dependencies": {
    "next": "^12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
